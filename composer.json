{
    "name": "laravel-notification-channels/laravel-jsk-sms-notification",
    "description": "laravel notification package for jsk sms notification",
    "homepage": "https://github.com/laravel-notification-channels/laravel-jsk-sms-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Ramananda Panda",
            "email": "ramanandapanda@hotmail.com",
            "homepage": "https://ramanandapanda.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\JSKSMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\JSKSMS\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
